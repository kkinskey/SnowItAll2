<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="342.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.snowitall.GradeK1.GeometryController">
    <children>
        <ImageView fitHeight="390.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@backgrounds/QuestionType.png" />
            </image>
        </ImageView>
        <Button layoutX="189.0" layoutY="263.0" mnemonicParsing="false" onAction="#handleFIB" opacity="0.0" prefHeight="42.0" prefWidth="223.0" text="Fill in the Blank">
            <cursor>
                <Cursor fx:constant="OPEN_HAND" />
            </cursor>
        </Button>
        <Button layoutY="284.0" mnemonicParsing="false" onAction="#returnToGeometryButton" opacity="0.0" prefHeight="53.0" prefWidth="88.0" text="return">
            <cursor>
                <Cursor fx:constant="OPEN_HAND" />
            </cursor>
        </Button>
    </children>

</AnchorPane>
